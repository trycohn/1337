{
    "headers": [
      {
        "source": "/manifest.json",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "application/json" }
        ]
      },
      {
        "source": "/logo192.png",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "image/png" }
        ]
      },
      {
        "source": "/logo512.png",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "image/png" }
        ]
      }
    ],
    "rewrites": [
      { "source": "/chat", "destination": "https://api.1337community.com/chat" },
      { "source": "/ws", "destination": "https://api.1337community.com/ws" },
      { "source": "/api/:path*", "destination": "https://api.1337community.com/api/:path*" }
    ]
  }